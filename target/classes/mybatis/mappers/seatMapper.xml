<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="seatMapper">
	<insert id="seatBooking" parameterType="SeatVO">
		insert into seat values (seat_seq.nextval,#{theater_num},#{cinema_num},#{seat_check},#{seat_num})
	</insert>
	<select id="getTheaterNum" resultType="CinemaVO">
		select cinema_loc,cinema_name from cinema where cinema_num = 1
	</select>
</mapper>